  <script src="~/js/QuestionsContainer/controllers.js"></script>
  <script src="~/js/QuestionsContainer/directives.js"></script>
  <script src="~/js/QuestionsContainer/services.js"></script>
  <script src="~/js/Helpers.js"></script>
  <script src="~/js/ShapesOverImage/services.js"></script>
  <script src="~/js/ShapesOverImage/directives.js"></script>
  <script src="~/js/ShapesOverImage/shapesOverImage.js"></script>
}

